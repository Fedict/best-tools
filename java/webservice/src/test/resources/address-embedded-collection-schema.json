{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Address collections schema",
	"type": "object",
	"properties": {
		"self": {
			"type": "string"
		},
		"items": {
			"type": "array",
			"minItems": 1,
			"items": {
				"type": "object",
				"properties": {
					"id": {
						"type": "string"
					},
					"self": {
						"type": "string"
					},
					"street": {
						"type": "object",
						"properties": {
							"id": {
								"type": "string"
							},
							"href": {
								"type": "string"
							}
						},
						"required": ["id", "href"]
					},
					"municipality": {
						"type": "object",
						"properties": {
							"id": {
								"type": "string"
							},
							"href": {
								"type": "string"
							}
						},
						"required": ["id", "href"]
					},
					"postalInfo": {
						"type": "object",
						"properties": {
							"id": {
								"type": "string"
							},
							"href": {
								"type": "string"
							}
						},
						"required": ["id", "href"]
					}
				},
				"houseNumber": {
					"type": "string"
				},
				"addressStatus": {
					"type": "string"
				},
				"startDate" : {
					"type": "date"
				},
				"endDate": {
					"type": "string"
				},
				"lambert72": {
					"type": "object",
					"properties": {
						"x": { "type": "double" },
						"y": { "type": "double" }
					},
					"required": ["x", "y"]
				},
				"gps": {
					"type": "object",
					"properties": {
						"x": { "type": "double" },
						"y": { "type": "double" }
					},
					"required": ["x", "y"]
				},
				"required": ["id", "self", "street", "municipality", "postal", "houseNumber", "addressStatus"]
			}
		}
	},
	"required": ["self", "items"]
}