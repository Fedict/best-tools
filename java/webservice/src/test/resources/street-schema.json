{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Street schema",
	"type": "object",
	"properties": {
		"id": {
			"type": "string"
		},
		"self": {
			"type": "string"
		},
		"name": {
			"type": "object",
			"properties": {
				"nl": {
					"type": "string"
				},
				"fr": {
					"type": "string"
				},
				"de": {
					"type": "string"
				}
			}
		},
		"validFrom": {
			"type": "string"
		},
		"validTill": {
			"type": "string"
		},
		"status": {
			"type": "string"
		}
	},
	"required": ["id", "self", "name", "status"]
}