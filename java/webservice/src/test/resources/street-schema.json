{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Street schema",
	"type": "object",
	"properties": {
		"id": {
			"type": "string"
		},
		"self": {
			"type": "string"
		},
		"streetName": {
			"type": "object",
			"properties": {
				"nl": {
					"type": "string"
				},
				"fr": {
					"type": "string"
				},
				"de": {
					"type": "string"
				}
			}
		},
		"beginDate": {
			"type": "string"
		},
		"endDate": {
			"type": "string"
		},
		"streetNameStatus": {
			"type": "string"
		}
	},
	"required": ["id", "self", "streetName", "streetNameStatus"]
}