{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Address collection schema",
	"type": "object",
	"properties": {
		"self": {
			"type": "string"
		},
		"items": {
			"type": "array",
			"minItems": 1,
			"items": {
				"type": "object",
				"properties": {
					"id": {
						"type": "string"
					},
					"self": {
						"type": "string"
					},
					"street": {
						"type": "object",
						"properties": {
							"id": {
								"type": "string"
							},
							"href": {
								"type": "string"
							}
						},
						"required": ["id", "href"]
					},
					"municipality": {
						"type": "object",
						"properties": {
							"id": {
								"type": "string"
							},
							"href": {
								"type": "string"
							}
						},
						"required": ["id", "href"]
					},
					"postal": {
						"type": "object",
						"properties": {
							"id": {
								"type": "string"
							},
							"href": {
								"type": "string"
							}
						},
						"required": ["id", "href"]
					},
					"houseNumber": {
						"type": "string"
					},
					"status": {
						"type": "string"
					},
					"validFrom": {
						"type": "string"
					},
					"lambert72": {
						"type": "object",
						"properties": {
							"x": {
								"type": "number"
							},
							"y": {
								"type": "number"
							}
						},
						"required": ["x", "y"]
					},
					"gps": {
						"type": "object",
						"properties": {
							"x": {
								"type": "number"
							},
							"y": {
								"type": "number"
							}
						},
						"required": ["x", "y"]
					}
				},
				"required": ["id", "self", "street", "municipality", "postal", "housenumber", "status"]
			}
		},
		"pageSize": {
			"type": "integer"		
		},
		"first": {
			"type": "string"
		}
	},
	"required": ["self", "items"]
}