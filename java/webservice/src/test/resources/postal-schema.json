{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Postal info schema",
	"type": "object",
	"properties": {
		"id": {
			"type": "string"
		},
		"self": {
			"type": "string"
		},
		"postalCode": {
			"type": "string"
		},
		"name": {
			"type": "object",
			"properties": {
				"nl": {
					"type": "string"
				},
				"fr": {
					"type": "string"
				},
				"de": {
					"type": "string"
				}
			}
		}
	},
	"required": ["id", "self", "postalCode"]
}